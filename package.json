{
  "name": "pathfinder",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "cookie-parser": "^1.4.5",
    "node-sass": "^4.13.1",
    "npm-watch": "^0.6.0",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.0",
    "react-chat-widget": "^3.0.0-beta.2",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "react-modal": "^3.0.3",
    "recharts": "^1.0.0-alpha.6",
    "styled-components": "^2.2.0",
    "shards-react": "^1.0.3",
    "animejs": "^2.2.0",
    "prop-types": "^15.6.0"
  },
  "devDependencies": {},
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js,jsx"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "watch": "npm-watch",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
